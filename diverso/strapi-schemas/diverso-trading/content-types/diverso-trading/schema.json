{
  "kind": "collectionType",
  "collectionName": "diverso_tradings",
  "info": {
    "singularName": "diverso-trading",
    "pluralName": "diverso-tradings",
    "displayName": "Diverso Trading",
    "description": "Trading/commercial services page content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "metaDescription": {
      "type": "text",
      "required": true,
      "maxLength": 160
    },
    "metaImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "metaKeywords": {
      "type": "string",
      "required": false
    },
    "metaRobots": {
      "type": "string",
      "required": false,
      "default": "index, follow"
    },
    "metaUrl": {
      "type": "string",
      "required": false
    },
    "pageTitle": {
      "type": "string",
      "required": true
    },
    "pageDescription": {
      "type": "text",
      "required": true
    },
    "headerQuote": {
      "type": "text",
      "required": true
    },
    "mainTitle": {
      "type": "string",
      "required": true
    },
    "mainContent": {
      "type": "richtext",
      "required": true
    },
    "organizationsList": {
      "type": "component",
      "repeatable": true,
      "component": "diverso.organization"
    },
    "projects": {
      "type": "component",
      "repeatable": true,
      "component": "diverso.project"
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    }
  }
}
